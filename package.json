  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build",
    "package.json",
    "package-lock.json",
    "vite.config.mjs",
    ".eslintrc.js",
    "tailwind.config.ts",
    "lint-staged.config.js",
    "postcss.config.js",
    "prettier.config.js",
    "dist",
    ".vercel",
    ".cache"
